{
  "name": "node-express-es6-sqlite-api-server-starter",
  "version": "0.1.0",
  "description": "A simple NodeJs starter template for creating ExpressJs based Api Server using SqlLite as backend. Uses ES6 with async+await through Babel.",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src \"",
    "build": "babel src -s -D -d dist",
    "start": "node dist",
    "prestart": "npm run -s build",
    "test": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": "https://github.com/theskhan/node-express-es6-sqlite-api-server-starter.git",
  "author": "Suhel Khan",
  "license": "MIT",
"keywords": [
	"nodejs",
	"es6",
	"express",
	"express-starter",
	"node-sqllite-express-api-server-starter"
  ],  
  "dependencies": {
    "babel-polyfill": "^6.20.0",
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "express": "^4.13.3",
    "resource-router-middleware": "^0.5.1",
    "sqlite": "^2.2.4"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2017": "^6.16.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^3.1.1",
    "nodemon": "^1.9.2"
  }
}
